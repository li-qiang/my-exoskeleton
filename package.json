{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "my-exoskeleton",
  "title": "My Exoskeleton",
  "description": "Development Tools",
  "icon": "project-manage_2.png",
  "author": "Qlee",
  "categories": [
    "Developer Tools"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "My Project",
      "description": "My Project List",
      "mode": "view",
      "icon": "project-manage_2.png"
    },
    {
      "name": "gocd-pipelines",
      "title": "GoCD Pipelines",
      "description": "GoCD Pipelines List And Details",
      "mode": "view",
      "icon": "command-gocd-icon.png"
    },
    {
      "name": "grafana-quick",
      "title": "Grafana",
      "description": "Grafana Quick Link",
      "mode": "view",
      "icon": "command-grafana-icon.png"
    },
    {
      "name": "dictionary",
      "title": "Dictionary",
      "description": "Project Dictionary",
      "mode": "view",
      "icon": "dictionary.png"
    },
    {
      "name": "update-version",
      "title": "Update Version",
      "description": "Update Version",
      "mode": "view",
      "icon": "update.png"
    }
  ],
  "preferences": [
    {
      "name": "GocdPAT",
      "title": "GOCD PAT",
      "description": "GOCD Personal Access Token",
      "placeholder": "GOCD Personal Access Token",
      "type": "textfield",
      "required": true
    },
    {
      "name": "GoCDBaseUrl",
      "title": "GOCD Base Url",
      "description": "GOCD Base Url",
      "placeholder": "https://ci.example.com",
      "type": "textfield",
      "required": true
    },
    {
      "name": "GrafanaPAT",
      "title": "Grafana PAT",
      "description": "Grafana API Keys",
      "placeholder": "Grafana API Keys",
      "type": "textfield",
      "required": true
    },
    {
      "name": "GrafanaBaseUrl",
      "title": "Grafana Base URL",
      "description": "Grafana Base Url",
      "type": "textfield",
      "placeholder": "https://monitor.example.com",
      "required": true
    },
    {
      "name": "dictionaryUrl",
      "title": "MySQL Connection For Dictionary",
      "description": "dictionary database url",
      "placeholder": "mysqlx://username:password@ip:port",
      "type": "textfield",
      "required": true
    },
    {
      "name": "githubToken",
      "title": "Github Personal Access Token",
      "description": "Github Personal Access Token",
      "placeholder": "Enter Your Github Token",
      "type": "textfield",
      "required": true
    }
  ],
  "dependencies": {
    "@mysql/xdevapi": "^8.0.32",
    "@raycast/api": "^1.47.3",
    "@raycast/utils": "^1.4.16",
    "@types/mysql": "^2.15.21",
    "axios": "^1.3.3",
    "csv-parse": "^5.3.5",
    "googleapis": "^105.0.0",
    "moment": "^2.29.4",
    "mysql": "^2.18.1",
    "node-fetch": "^3.3.0",
    "sqlite3": "^5.1.4",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@types/node": "18.8.3",
    "@types/react": "18.0.9",
    "@types/xlsx": "^0.0.36",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "prettier": "^2.5.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist -o dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "ray publish"
  }
}
